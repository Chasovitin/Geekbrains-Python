print(int(num_3))

# задача №3


num_1: int = 0

num_2: int = 18

number = int(input("сколько вам лет?"))
if num_1 < number <  num_2:

    print("Извините, пользование данным ресурсом только с 18 лет")
else:
    print("доступ разрешен")



